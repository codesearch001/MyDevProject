<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:shape="oval">
            <solid android:color="@color/toggle_off_color"/>
            <stroke android:width="2dp" android:color="@color/toggle_on_color"/>
        <size android:width="23dp" android:height="23dp"/>
        </shape>
    </item>


    <item android:state_checked="false">
        <shape android:shape="oval">
            <solid android:color="@color/white"/>
            <stroke android:width="2dp" android:color="@color/unChecked_thumb"/>
            <size android:width="23dp" android:height="23dp"/>
        </shape>
    </item>
</selector>